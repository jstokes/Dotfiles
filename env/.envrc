# environment settings

ZSH=$HOME/.oh-my-zsh
ZSH_THEME="sorin"

export M2_HOME=$HOME/Developer/maven/

hash -d tomcat=/Library/Tomcat/

autoload -U colors


# Uncomment following line if you want to disable autosetting terminal title.
DISABLE_AUTO_TITLE="true"

# Uncomment following line if you want red dots to be displayed while waiting for completion
COMPLETION_WAITING_DOTS="true"

plugins=(bundler git git-flow ruby rails homebrew gem mvn osx rake rvm)

source $ZSH/oh-my-zsh.sh

if [[ -s "$HOME/.rvm/scripts/rvm" ]] ; then source "$HOME/.rvm/scripts/rvm" ; fi

export PATH=/Library/Frameworks/Python.framework/Versions/2.7/bin:/Library/Frameworks/Python.framework/Versions/2.7/bin:~/.rvm/gems/ruby-1.9.2-p290/bin:~/.rvm/gems/ruby-1.9.2-p290@global/bin:~/.rvm/rubies/ruby-1.9.2-p290/bin:~/.rvm/bin:/usr/local/bin:/bin:/usr/sbin:/sbin:/usr/bin:~/Developer/maven/bin:~/Developer/mongo2.2/bin:/usr/X11/bin:/usr/local/git/bin:~/Developer/maven/bin:~/Developer/maven/bin:~/Developer/bin:~/.rvm/bin:~/.rvm/rubies/ruby-1.9.3-p194/bin:~/.rvm/gems/ruby-1.9.3-p194/bin:~/.rvm/gems/ruby-1.9.3-p194@global/bin
export PATH="${PATH}:/Applications/apache-jmeter-2.8/bin"
export PATH=/usr/local/mysql/bin:$PATH
launchctl setenv PATH $PATH
