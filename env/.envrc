export PATH="/sbin":$PATH
export PATH="/usr/sbin":$PATH
export PATH="/bin":$PATH
export PATH="/usr/bin":$PATH
export PATH="/usr/local/bin":$PATH
export PATH="$HOME/Developer/bin":$PATH
export PATH="/usr/local/git/bin":$PATH
export PATH="/usr/X11/bin":$PATH
export PATH="/usr/local/share/npm/bin":$PATH
export PATH="/usr/local/share/npm/bin":$PATH
export PATH="/usr/local/mysql/bin":$PATH
export PATH="/usr/local/share/npm/lib/node_modules":$PATH
export PATH="/usr/local/opt/ruby/bin":$PATH
export PATH="/Users/jeff/.emacs.d/bin":$PATH
export TERM="screen-256color"
export LEIN_SNAPSHOTS_IN_RELEASE="true"
export EDITOR="nvim"
export VISUAL="less"
export LESS="-F -g -i -M -r -+S -w -X -z-4"

export M2_HOME="`brew --prefix maven`/libexec"
export JAVA_HOME=$(/usr/libexec/java_home -v 11)

export JAVA_OPTS="$JAVA_OPTS -XX:+FlightRecorder -XX:-OmitStackTraceInFastThrow -XX:+UnlockDiagnosticVMOptions -XX:+DebugNonSafepoints"

HISTCONTROL=ignoreboth
HISTIGNORE='ls:bg:fg:history'
PROMPT_COMMAND='history -a'

hash -d -- play="$HOME/play"
hash -d -- work="$HOME/work"
