[user]
  name = Jeff 
  email = jeffecu88@gmail.com
[core]
  excludesfile = ~/.gitignore_global
  repositoryformatversion = 0
  filemode = true
  bare = false
  logallrefupdates = true
  ignorecase = true
  editor = vim 
[alias]
  us = reset HEAD
  uc = reset --soft HEAD^
  pl = smart-pull 
  ph = push
  st = status -s
  co = checkout
  ci = commit
  br = branch
  df = diff
  dfs = diff --stat
  dc = diff --cached
  aa = add -A
  ai = add -i
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit 
  rh = reset --hard
  m = merge
  cb = checkout -b
[color]
  ui = true
[push]
  default = current
